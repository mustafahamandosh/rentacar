{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "paths": {
      "models/*": [
        "modules/cars/model/*",
        "modules/accounts/model/*"
      ],
      "controllers/*": [
        "modules/cars/useCases/*",
        "modules/accounts/useCases/*"
      ],
      "usecases/*": [
        "modules/cars/useCases/*",
        "modules/accounts/useCases/*"
      ],
      "repository/*": [
        "modules/cars/repository/implementations/*",
        "modules/accounts/repository/implementations/*"
      ],
      "repository-interface/*": [
        "modules/cars/repository/*",
        "modules/accounts/repository/*"
      ],
      "dto/*": [
        "modules/cars/dtos/*",
        "modules/accounts/dtos/*"
      ],
      "routes/*": [
        "routes/*"
      ],
      "errors/*": [
        "errors/*"
      ],
      "utils/*": [
        "utils/*"
      ]
    }
  }
}
